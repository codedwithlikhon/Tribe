{
  "name": "tribe-ai-chatbot",
  "version": "1.0.0",
  "description": "AI-powered chatbot that combines Gemini models with the Tribe WebContainer runtime for in-browser code execution.",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:client\"",
    "dev:client": "vite --host",
    "dev:server": "tsx watch server/index.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "tsc --project tsconfig.server.json",
    "preview": "npm run build && concurrently \"node dist/server/index.js\" \"vite preview --host\"",
    "lint": "eslint .",
    "check:maintenance": "tsx scripts/check-maintenance.ts"
  },
  "dependencies": {
    "@ai-sdk/cohere": "^2.0.12",
    "@ai-sdk/groq": "^2.0.22",
    "@types/node": "^20.11.30",
    "@webcontainer/api": "^1.1.9",
    "@xyflow/react": "^12.8.6",
    "ai": "^3.2.15",
    "ai-sdk-provider-gemini-cli": "^1.1.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "lucide-react": "^0.545.0",
    "morgan": "^1.10.0",
    "path-browserify": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@eslint/js": "^9.37.0",
    "@types/morgan": "^1.9.5",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.0",
    "eslint": "^9.37.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.1.1",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.46.0",
    "vite": "^5.1.6"
  }
}
